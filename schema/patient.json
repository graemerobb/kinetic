{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Patient",
  "type": "object",
  "properties": {
    "id": {
      "type": "string",
      "description": "Unique patient identifier"
    },
     "clinic_id": {
      "type": "string",
      "description": "References Clinic.id"
    },
    "email": {
      "type": "string",
      "format": "email"
    },
    "address": {
      "type": "string"
    },
    "created_at": {
      "type": "string",
      "format": "date-time"
    }
  },
  "required": ["id", "clinic_id", "email", "created_at"],
  "additionalProperties": false
}
